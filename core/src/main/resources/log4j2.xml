<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<!-- CONSOLE -->
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <!-- LOGGER -->
  <Loggers>
    <Logger name="test.util.R" level="trace" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="gospl.algo.ipf.margin.MarginalsIPFProcessor" level="info" additivity="false">
    	<AppenderRef ref="Console"/>
    </Logger>
    <Root level="debug">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
